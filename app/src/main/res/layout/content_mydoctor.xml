<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_mydoctor"
        tools:context=".mydoctor">
    <EditText
            android:layout_width="320dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText9" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="109dp"
            app:layout_constraintTop_toTopOf="parent"/>
    <EditText
            android:layout_width="320dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText10" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText9" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="320dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText11" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText10" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="320dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText12" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText11" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="320dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText13" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText12" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <EditText
            android:layout_width="320dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText14" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText13" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="Update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button5" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="150dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="150dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/editText14" android:layout_marginBottom="110dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/roundbutton"/>
</android.support.constraint.ConstraintLayout>